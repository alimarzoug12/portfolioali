{
  "name": "portfolioali",
  "private": true,
  "scripts": {
    "build": "npm run build:frontend && npm run build:portfolio && node copy.js",
    "build:frontend": "cd frontend && ../node_modules/.bin/react-scripts build",
    "build:portfolio": "cd portfolio && ../node_modules/.bin/react-scripts build",
    "dev": "concurrently \"cd frontend && ../node_modules/.bin/react-scripts start\" \"cd portfolio && ../node_modules/.bin/react-scripts start\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-router-dom": "^6.14.2",
    "react-router-hash-link": "^2.4.3",
    "react-waypoint": "^10.3.0",
    "bootstrap-icons": "^1.13.1",
    "@emailjs/browser": "^4.4.1",
    "fs-extra": "^11.1.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^13.5.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}