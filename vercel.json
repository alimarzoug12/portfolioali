{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/portfolio",
      "destination": "/portfolio/index.html"
    },
    {
      "source": "/portfolio/:path*",
      "destination": "/portfolio/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}